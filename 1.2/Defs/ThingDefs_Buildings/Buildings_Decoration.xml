<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>ED_Doormat</defName>
		<label>doormat</label>
		<description>A decorative mat, manufactured from sturdy materials, that adds a nice, welcoming touch to any room.</description>
		<graphicData>
			<texPath>Things/Building/ED_DoorMat/ED_DoorMatA</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>3</costStuffCount>		
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
			<isEdifice>false</isEdifice>
		</building>
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="VanillaFurnitureExpanded.CompProperties_RandomBuildingGraphic">
				<randomGraphics>
					<li>Things/Building/ED_DoorMat/ED_DoorMatA</li>
					<li>Things/Building/ED_DoorMat/ED_DoorMatB</li>

				</randomGraphics>
			</li>
		</comps>		
		<statBases>
			<Beauty>1</Beauty>
			<Flammability>1.0</Flammability>
			<MarketValue>5</MarketValue>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToBuild>50</WorkToBuild>
			<Mass>0.25</Mass>
		</statBases>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<clearBuildingArea>false</clearBuildingArea>
		<designationCategory>Furniture</designationCategory>

		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<minifiedDef>MinifiedThing</minifiedDef>
		<passability>Standable</passability>
		<pathCost>0</pathCost>
		<repairEffect>Repair</repairEffect>
		<rotatable>true</rotatable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<tickerType>Never</tickerType>
		<tradeTags>
			<li>Furniture</li>
		</tradeTags>
		<researchPrerequisites>
			<li>ED_SimpleDecorations</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="FurnitureBase">
