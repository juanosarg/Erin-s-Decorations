	<ThingDef ParentName="FurnitureWithQualityBase">
		<defName>ED_PetBowl</defName>
		<label>pet bowl</label>
		<description>A small metal dish typically used for feeding pet animals such as cats or dogs. Can be used to store kibble.</description>
		<thingClass>Building_Storage</thingClass>
		<graphicData>
			<texPath>Things/Building/ED_PetBowl</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.4</fillPercent>		
		<pathCost>5</pathCost>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>10</costStuffCount>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<Mass>1</Mass>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>0</Beauty>
		</statBases>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<fixedStorageSettings>
				<priority>Important</priority>
				<filter>
					<thingDefs>
						<li>Kibble</li>
					</thingDefs>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Important</priority>
				<filter>
					<thingDefs>
						<li>Kibble</li>
					</thingDefs>
				</filter>
			</defaultStorageSettings>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<designationHotKey>Misc12</designationHotKey>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<designationCategory>Furniture</designationCategory>
		<researchPrerequisites>
			<li>ED_SimpleDecorations</li>
		</researchPrerequisites>
	</ThingDef>

